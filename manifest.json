{
	"manifest_version": 2,
	"name": "SaferTwitter",
	"description": "Features for a safer twitter messaging experience",
	"version": "0.5",
	"permissions": [
		"*://twitter.com/"
	],
	"content_scripts": [
		{
			"matches": ["*://twitter.com/*"],
			"css": ["stylesheets/blur_dm_images.css"]
		}
	],
	"web_accessible_resources": [
		"stylesheets/*"
	]
}
